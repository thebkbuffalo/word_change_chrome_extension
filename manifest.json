{
"manifest_version": 2,
    "name": "Banality of Evil",
    "description": "Changes tent city or detention center to Concentration Camp on website",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*",
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
